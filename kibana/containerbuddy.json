{
  "consul": "{{ .CONSUL }}:8500",
  "onStart": ["/bin/manage.sh", "onStart"],
  "services": [
    {
      "name": "kibana",
      "port": 5601,
      "health": ["/bin/manage.sh", "health"],
      "poll": 10,
      "ttl": 25
    }
  ],
  "backends": [
    {
      "name": "elasticsearch",
      "poll": 5,
      "onChange": ["/bin/manage.sh", "reload"]
    }
  ]
}
